PREFIX v: <http://rdfs.org/ns/void#>
PREFIX e: <http://rdfs.org/ns/void#>

CONSTRUCT {
  <env://D>
    v:propertyPartition ?l .

  ?l
    v:property ?p ;
    s:subjectTypes ?k .

  ?k
    s:subjectClass ?t ;
    s:distinctMembers ?x
}
{
  SELECT
    (IRI(CONCAT('pp://', ENCODE_FOR_URI(STR(?p)))) AS ?l)
    (IRI(CONCAT('ppcp://', ENCODE_FOR_URI(STR(?p)), '-', ENCODE_FOR_URI(STR(?t)))) AS ?k)
    (COUNT(?o) AS ?x)
    ?p
    ?t
  {
    ?s
      a ?t ;
      ?p ?o
  }
  GROUP BY ?p ?t
}